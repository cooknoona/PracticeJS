let map = new Map();
map.set("name", "유나");
map.set("email", "yuna@gmail.com");
map.set("addr", "경기도 수원시");

console.log(map.size);
console.log(map.get("name"));
console.log(map.has("email"));
map.delete("name");

map.forEach(function (item){
    console.log(item);
});
